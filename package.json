{
  "name": "storefront_backend",
  "version": "0.1.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "jasmine": "jasmine",
    "build": "npx tsc",
    "start": "node dist/server.js",
    "test": "db-migrate --env test up && npm run build && npm run jasmine && db-migrate --env test down",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess 'nodemon ./dist/server.js --watch 'src/**''",
    "tsc": "tsc",
    "prettier": "prettier src/**/*.ts",
    "prettier-fix": "prettier --write src/**/*.ts",
    "lint": "npx eslint src/**/*.ts",
    "lint-fix": "npx eslint --fix src/**/*.ts",
    "nodemon": "npx nodemon --watch 'src/**' --ext 'ts,json' --ignore 'src/tests/*.ts' --exec 'ts-node src/server.ts --outDir ./dist'"
  },
  "author": "Udacity",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^16.0.2",
    "express": "^4.17.1",
    "express-validator": "^6.14.2",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.8.0",
    "typescript": "^4.8.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.9",
    "@types/express-validator": "^3.0.0",
    "@types/jasmine": "^3.6.4",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/pg": "^8.6.5",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jasmine": "^3.6.4",
    "jasmine-spec-reporter": "^6.0.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "supertest": "^6.2.4",
    "ts-node": "^10.9.1",
    "tsc-watch": "^4.6.2"
  }
